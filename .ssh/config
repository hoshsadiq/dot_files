Host *
    KeepAlive yes
    ServerAliveInterval 60
    PubkeyAuthentication yes
    IdentitiesOnly yes
    ForwardAgent no
    ForwardX11 no
    ForwardX11Trusted no
    Protocol 2

########## personal ##########
Host bitbucket.org
    user git
    HostName bitbucket.org
    IdentityFile ~/.ssh/id_rsa_bitbucket_private

Host github.com
    user git
    HostName github.com
    IdentityFile ~/.ssh/id_rsa_github_private

Host pi
    user osmc
    HostName osmc.co
    IdentityFile ~/.ssh/id_rsa_osmc
