export PROXY_SKIP="*.proxy.skip"
export PROXY_USERNAME="$(whoami)"
export PROXY_PASSWORD="passwd"
export PROXY_HOST="proxy-host:8080"

export DOT_FILES="$HOME/dot_files"
export JAVA_HOME='/usr/lib/jvm/java-11-openjdk-amd64'

source $HOME/dot_files/config/zsh/zshrc

addpath "$HOME/.local/bin" after
addpath /usr/lib/go-1.11/bin after
